#include<stdio.h>
#include<stdlib.h>
int main()
{
    int *a=0,*b=0,*c=0;
    a=(int *)malloc(sizeof(int));
    b=(int *)malloc(sizeof(int));
    c=(int *)malloc(sizeof(int));
    scanf("%d%d%d",a,b,c);
    
    if((*a>*b)&&(*a>*c) {
          printf("%d",*a);
      }
      
    else if((*b>*a)&&(*b>*c)) {
          printf("%d",*b);
      }
      
    else {
        printf("%d",*c);
    }
    return 0;
}

